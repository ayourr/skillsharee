    "babel-preset-stage-2": "^6.3.13",
    "babel-register": "^6.3.13",
    "mocha": "^2.3.4",
    "nodemon": "^1.8.1"
  },
  "dependencies": {
    "mongorito": "^2.1.2"
  }
}
